{
  "hosting": [
    {
      "site": "sentrei-beta-web",
      "appAssociation": "AUTO",
      "public": "public",
      "ignore": ["**/static/**"],
      "target": "web",
      "rewrites": [
        {
          "source": "**",
          "run": {
            "serviceId": "sentrei-beta-web",
            "region": "us-central1"
          }
        }
      ]
    },
    {
      "site": "sentrei-staging-beta-web",
      "public": "public",
      "ignore": ["**/static/**"],
      "target": "staging-web",
      "rewrites": [
        {
          "source": "**",
          "run": {
            "serviceId": "sentrei-staging-beta-web",
            "region": "us-central1"
          }
        }
      ]
    }
  ]
}
