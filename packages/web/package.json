  "scripts": {
    "alpha": "yarn run swap:alpha && firebase use alpha && yarn run deploy",
    "alpha:local": "yarn run swap:alpha && firebase use alpha",
    "analyze": "cross-env ANALYZE=true next build",
    "beta": "yarn run swap:beta && firebase use beta && yarn run deploy",
    "beta:local": "yarn run swap:beta && firebase use beta",
    "build": "next build",
    "cypress": "cypress run",
    "cypress:install": "cypress install",
    "deploy": "yarn run deploy:staging-web && yarn run deploy:staging-web",
    "deploy:staging-web": "firebase deploy --only hosting:staging-web",
    "deploy:web": "firebase deploy --only hosting:web",
    "dev": "next",
    "export": "next export",
    "firebase:alpha": "yarn run alpha:local && yarn run dev",
    "firebase:beta": "yarn run beta:local && yarn run dev",
    "firebase:master": "yarn run master:local && yarn run dev",
    "master": "yarn run swap:master && firebase use master && yarn run deploy",
    "master:local": "yarn run swap:master && firebase use master",
    "relative-ci": "relative-ci-agent",
    "sentry": "sentry-cli releases new -p sentrei ${SENTRY_RELEASE} && sentry-cli releases set-commits --auto ${SENTRY_RELEASE} && sentry-cli releases finalize ${SENTRY_RELEASE} && sentry-cli releases deploys ${SENTRY_RELEASE} new -e ${SENTRY_ENVIRONMENT}",
    "staging:alpha": "yarn run swap:alpha && firebase use alpha && yarn run deploy:staging-web",
    "staging:beta": "yarn run swap:beta && firebase use beta && yarn run deploy:staging-web",
    "staging:master": "yarn run swap:master && firebase use master && yarn run deploy:staging-web",
    "staging:storybook": "yarn run swap:master && firebase use master && yarn run deploy:staging-storybook",
    "start": "next start",
    "swap:alpha": "cp alpha.firebase.json firebase.json",
    "swap:beta": "cp beta.firebase.json firebase.json",
    "swap:master": "cp master.firebase.json firebase.json",
    "test": "start-server-and-test start http://localhost:3000 cypress"
  },
