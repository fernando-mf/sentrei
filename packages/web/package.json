{
  "name": "@sentrei/web",
  "version": "4.0.0",
  "homepage": "https://sentrei.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/sentrei/sentrei.git",
    "directory": "sentrei/web"
  },
  "license": "MIT",
  "author": "Shun Kakinoki",
  "scripts": {
    "alpha": "yarn run swap-alpha && firebase use alpha && yarn run deploy-web && yarn run deploy-staging-web",
    "analyze": "cross-env ANALYZE=true next build",
    "beta": "yarn run swap-beta && firebase use beta && yarn run deploy-web && yarn run deploy-staging-web",
    "build": "next build",
    "cypress": "cypress run",
    "deploy-staging-web": "firebase deploy --only hosting:staging-web --token $FIREBASE_TOKEN",
    "deploy-web": "firebase deploy --only hosting:web --token $FIREBASE_TOKEN",
    "dev": "next",
    "export": "next export",
    "master": "yarn run swap-master && firebase use master && yarn run deploy-web",
    "staging-alpha": "yarn run swap-alpha && firebase use alpha && yarn run deploy-staging-web",
    "staging-beta": "yarn run swap-beta && firebase use beta && yarn run deploy-staging-web",
    "staging-master": "yarn run swap-master && firebase use master && yarn run deploy-staging-web",
    "staging-storybook": "yarn run swap-master && firebase use master && yarn run deploy-staging-storybook",
    "start": "next start",
    "swap-alpha": "cp alpha.firebase.json firebase.json",
    "swap-beta": "cp beta.firebase.json firebase.json",
    "swap-master": "cp master.firebase.json firebase.json",
    "test": "start-server-and-test start http://localhost:3000 cypress"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.10",
    "@next/bundle-analyzer": "^9.3.5",
    "@sentrei/ui": "*",
    "@storybook/react": "^5.3.18",
    "@stripe/react-stripe-js": "^1.1.2",
    "@stripe/stripe-js": "^1.4.0",
    "next": "^9.3.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "recharts": "^1.8.5",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.5.3",
    "@cypress/webpack-preprocessor": "^5.1.2",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/next": "^9.0.0",
    "@types/node": "^13.13.0",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "awesome-typescript-loader": "^5.2.1",
    "babel-plugin-istanbul": "^6.0.0",
    "cross-env": "^7.0.2",
    "cypress": "^4.4.1",
    "start-server-and-test": "^1.11.0",
    "ts-node": "^8.9.0",
    "typescript": "^3.8.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
