  "scripts": {
    "build": "build-storybook -o out",
    "deploy:staging-storybook": "firebase deploy --only hosting:staging-ui --token $FIREBASE_TOKEN",
    "deploy:storybook": "firebase deploy --only hosting:ui --token $FIREBASE_TOKEN",
    "dev": "start-storybook -p 6006",
    "fix": "yarn run eslint-fix",
    "jest": "jest --coverage",
    "staging:ui": "firebase use master && yarn run deploy:staging-storybook",
    "storycap": "storycap --serverCmd 'start-storybook -p 9001' http://localhost:9001 --serverTimeout 120000",
    "test": "yarn run jest",
    "typescript": "tsc -p tsconfig.json",
    "ui": "firebase use master && yarn run deploy:storybook"
  },
