{
  "hosting": [
    {
      "site": "sentrei-beta-dartdoc",
      "target": "dartdoc",
      "public": "doc/api",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    },
    {
      "site": "sentrei-staging-beta-dartdoc",
      "target": "staging-dartdoc",
      "public": "doc/api",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    }
  ]
}
