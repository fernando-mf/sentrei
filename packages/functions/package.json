  "scripts": {
    "alpha": "yarn run swap:alpha && firebase use alpha && yarn run deploy:rules",
    "beta": "yarn run swap:beta && firebase use beta && yarn run deploy:rules",
    "deploy:rules": "firebase deploy --only firestore:rules --token ${FIREBASE_TOKEN}",
    "master": "yarn run swap:master && firebase use master && yarn run deploy:rules",
    "swap:alpha": "cp alpha.firebase.json firebase.json",
    "swap:beta": "cp beta.firebase.json firebase.json",
    "swap:master": "cp master.firebase.json firebase.json"
  },
