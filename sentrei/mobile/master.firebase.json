{
  "hosting": [{
      "site": "sentrei-master-mobile",
      "target": "mobile",
      "public": "doc/api",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
      "rewrites": [{
        "source": "**",
        "destination": "/index.html"
      }]
    },
    {
      "site": "sentrei-staging-master-mobile",
      "target": "staging-mobile",
      "public": "doc/api",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
      "rewrites": [{
        "source": "**",
        "destination": "/index.html"
      }]
    }
  ]
}
