{
  "preset": "ts-jest",
  "transform": {
    "\\.mdx$": "@storybook/addon-docs/jest-transform-mdx",
    "\\.jsx?$": [
      "babel-jest",
      {
        "presets": [
          "@babel/preset-env",
          "@babel/preset-react"
        ]
      }
    ]
  },
  "coveragePathIgnorePatterns": [
    "/node_modules/",
    "/\\.storybook/",
    "\\.stories\\."
  ]
}
