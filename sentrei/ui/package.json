{
  "name": "@sentrei/ui",
  "version": "3.16.6",
  "homepage": "https://sentrei.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/sentrei/sentrei.git",
    "directory": "sentrei/ui"
  },
  "license": "MIT",
  "author": "Shun Kakinoki",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rimraf lib && tsc",
    "build-storybook": "build-storybook -o out",
    "deploy-staging-storybook": "firebase deploy --only hosting:staging-storybook --token $FIREBASE_TOKEN",
    "deploy-storybook": "firebase deploy --only hosting:storybook --token $FIREBASE_TOKEN",
    "eslint-check": "eslint --ext .js,.jsx,.ts,.tsx src",
    "eslint-fix": "eslint --fix --ext .js,.jsx,.ts,.tsx src",
    "fix": "yarn run eslint-fix",
    "jest": "jest --coverage",
    "staging-storybook": "yarn run swap-master && firebase use master && yarn run deploy-staging-storybook",
    "start": "start-storybook -p 6006",
    "storybook": "yarn run swap-master && firebase use master && yarn run deploy-storybook",
    "storycap": "storycap --serverCmd 'start-storybook -p 9001' http://localhost:9001 --serverTimeout 120000",
    "test": "yarn run eslint-check && yarn run jest",
    "watch": "tsc -w --preserveWatchOutput"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.10",
    "@next/bundle-analyzer": "^9.3.5",
    "@sentrei/common": "*",
    "@sentrei/ui": "*",
    "@storybook/react": "^5.3.18",
    "@stripe/react-stripe-js": "^1.1.2",
    "@stripe/stripe-js": "^1.4.0",
    "next": "^9.3.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "recharts": "^1.8.5",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^25.2.1",
    "@types/next": "^9.0.0",
    "@types/node": "^13.13.0",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/storybook__react": "^5.2.1",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "awesome-typescript-loader": "^5.2.1",
    "cross-env": "^7.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "jest": "^25.4.0",
    "jest-enzyme": "^7.1.2",
    "pretty-quick": "^2.0.1",
    "rimraf": "^3.0.2",
    "storycap": "^2.3.5",
    "ts-jest": "^25.4.0",
    "ts-loader": "^7.0.1",
    "typescript": "^3.8.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
