{
  "name": "@sentrei/web",
  "version": "3.16.3",
  "homepage": "https://sentrei.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/sentrei/sentrei.git",
    "directory": "sentrei/web"
  },
  "license": "MIT",
  "author": "Shun Kakinoki",
  "scripts": {
    "alpha": "yarn run swap-alpha && firebase use alpha && yarn run deploy-web",
    "analyze": "cross-env ANALYZE=true next build",
    "beta": "yarn run swap-beta && firebase use beta && yarn run deploy-web",
    "build": "next build",
    "build-storybook": "build-storybook",
    "deploy-staging-web": "firebase deploy --only hosting:staging-web --token $FIREBASE_TOKEN",
    "deploy-web": "firebase deploy --only hosting:web --token $FIREBASE_TOKEN",
    "dev": "next",
    "export": "next export",
    "master": "yarn run swap-master && firebase use master && yarn run deploy-web",
    "staging-alpha": "yarn run swap-alpha && firebase use alpha && yarn run deploy-staging-web",
    "staging-beta": "yarn run swap-beta && firebase use beta && yarn run deploy-staging-web",
    "staging-master": "yarn run swap-master && firebase use master && yarn run deploy-staging-web",
    "start": "next start -p 8080",
    "storybook": "start-storybook -p 6006",
    "swap-alpha": "cp alpha.firebase.json firebase.json",
    "swap-beta": "cp beta.firebase.json firebase.json",
    "swap-master": "cp master.firebase.json firebase.json"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.10",
    "@next/bundle-analyzer": "^9.3.5",
    "@sentrei/common": "^3.13.3",
    "@stripe/react-stripe-js": "^1.1.2",
    "@stripe/stripe-js": "^1.4.0",
    "next": "^9.3.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "recharts": "^1.8.5",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "@types/next": "^9.0.0",
    "@types/node": "^13.13.0",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/storybook__react": "^5.2.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.1.0",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.2",
    "rimraf": "^3.0.2",
    "ts-loader": "^7.0.1",
    "typescript": "^3.8.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
