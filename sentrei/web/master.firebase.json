{
  "hosting": [
    {
      "site": "sentrei-master-web",
      "public": "public",
      "target": "web",
      "rewrites": [
        {
          "source": "**",
          "run": {
            "serviceId": "sentrei-master-web",
            "region": "us-central1"
          }
        }
      ]
    },
    {
      "site": "sentrei-staging-master-web",
      "public": "public",
      "target": "staging-web",
      "rewrites": [
        {
          "source": "**",
          "run": {
            "serviceId": "sentrei-staging-master-web",
            "region": "us-central1"
          }
        }
      ]
    },
    {
      "site": "sentrei-storybook",
      "target": "storybook",
      "public": "out",
      "predeploy": ["yarn run build"],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    },
    {
      "site": "sentrei-staging-storybook",
      "target": "staging-storybook",
      "public": "out",
      "predeploy": ["yarn run build"],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    }
  ]
}
