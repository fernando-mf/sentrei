{
  "hosting": [
    {
      "site": "sentrei-alpha-web",
      "target": "web",
      "public": "dist/public",
      "rewrites": [
        {
          "source": "**/**",
          "function": "hosting"
        }
      ]
    },
    {
      "site": "sentrei-staging-alpha-web",
      "target": "staging-web",
      "public": "dist/public",
      "rewrites": [
        {
          "source": "**/**",
          "function": "stagingHosting"
        }
      ]
    }
  ]
}
