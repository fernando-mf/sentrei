root = true

[*]
indent_style = space
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.{json,mdx,yml,js,tf,ts,tsx}]
indent_size = 2

[Fastfile]
indent_size = 2

[*.{pbxproj,ts,tsx}]
indent_style = none
indent_size = none

[Makefile]
indent_style = tab

[*.{arb,flr,png,gpg,html}]
charset = none
end_of_line = none
insert_final_newline = none
trim_trailing_whitespace = none
indent_style = none
indent_size = none

[sentrei/mobile/android/firebase/**]
charset = none
end_of_line = none
insert_final_newline = none
trim_trailing_whitespace = none
indent_style = none
indent_size = none

[sentrei/mobile/android/key/**]
charset = none
end_of_line = none
insert_final_newline = none
trim_trailing_whitespace = none
indent_style = none
indent_size = none
